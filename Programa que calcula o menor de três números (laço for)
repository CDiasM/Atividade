#include <stdio.h>

int main() {
    int numeros[3], i, menor;

    printf("Digite três números:\n");
    for (i = 0; i < 3; i++) {
        scanf("%d", &numeros[i]);
    }

    menor = numeros[0];
    for (i = 1; i < 3; i++) {
        if (numeros[i] < menor) {
            menor = numeros[i];
        }
    }

    printf("O menor número é: %d\n", menor);
    return 0;
}
