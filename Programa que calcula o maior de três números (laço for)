#include <stdio.h>

int main() {
    int numeros[3], i, maior;

    printf("Digite três números:\n");
    for (i = 0; i < 3; i++) {
        scanf("%d", &numeros[i]);
    }

    maior = numeros[0];
    for (i = 1; i < 3; i++) {
        if (numeros[i] > maior) {
            maior = numeros[i];
        }
    }

    printf("O maior número é: %d\n", maior);
    return 0;
}
